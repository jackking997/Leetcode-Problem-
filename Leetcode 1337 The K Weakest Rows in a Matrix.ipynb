   "source": [
    "def kWeakestRows(mat, k):\n",
    "    count = {}        \n",
    "    for i in range(len(mat)):\n",
    "        count[i] = sum(mat[i])\n",
    "        \n",
    "    res = list(i for i in range(len(mat)))\n",
    "        \n",
    "    res.sort(key = lambda x : count[x])\n",
    "\n",
    "    return res[:k]\n",
    "        \n",
    "        \n",
    "        "
   ]
  }
